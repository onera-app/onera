{
  "name": "@onera/server",
  "version": "0.1.0",
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    }
  },
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "build": "bun build src/index.ts --outdir dist --target node --external libsodium-wrappers-sumo --external libsodium-sumo",
    "start": "node dist/index.js",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "bun run src/db/migrate.ts",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:seed-tee": "bun run src/db/seed-tee.ts",
    "db:seed-plans": "bun run src/db/seed-plans.ts",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@hono/node-server": "^1.13.8",
    "@hono/trpc-server": "0.4.2",
    "@onera/crypto": "workspace:*",
    "@simplewebauthn/server": "13.2.2",
    "@simplewebauthn/types": "12.0.0",
    "@supabase/supabase-js": "2.95.3",
    "@trpc/server": "^11.0.0-rc.730",
    "dodopayments": "2.19.0",
    "drizzle-orm": "^0.38.4",
    "hono": "^4.7.4",
    "pg": "^8.13.1",
    "socket.io": "^4.8.1",
    "ws": "8.19.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/bun": "^1.2.0",
    "@types/pg": "^8.11.10",
    "@types/ws": "8.18.1",
    "bun-types": "1.3.9",
    "drizzle-kit": "^0.30.4",
    "typescript": "^5.7.0"
  }
}
